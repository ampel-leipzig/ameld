title <- sub("AMPEL", "{AMPEL}", meta$Title)
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(
    bibtype = "Manual",
    title = title,
    author = c(person("Sebastian", "Gibb")),
    year = year,
    note = note,
	url = "https://github.com/ampel-leipzig/ameld",
    doi = "10.5281/zenodo.6666018"
)
